                                    <span class="pcoded-mcaret"></span>
                                </a>
                                <ul class="pcoded-submenu">
                                    <li class="pcoded-hasmenu">
                                        <a href="@Url.Action("Index","Group")">
                                            <span class="pcoded-mtext">Grup Listesi</span>
                                        </a>
                                    </li>
                                    <li class="pcoded-hasmenu">
                                        <a href="@Url.Action("AddGroup","Group")">
                                            <span class="pcoded-mtext">Grup Ekle</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li class="pcoded-hasmenu">
                                <a href="javascript:void(0)">
                                    <span class="pcoded-micon"><i class="feather icon-box"></i></span>
                                    <span class="pcoded-mtext">Yetki İşlemleri</span>
                                    <span class="pcoded-mcaret"></span>
                                </a>
                                <ul class="pcoded-submenu">
                                    <li class="pcoded-hasmenu">
                                        <a href="@Url.Action("Index","Authority")">
                                            <span class="pcoded-mtext">Yetki Listesi</span>
                                        </a>
                                    </li>
                                    <li class="pcoded-hasmenu">
                                        <a href="@Url.Action("AddAuthority","Authority")">
                                            <span class="pcoded-mtext">Yetki Ekle</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </nav>
                <div class="pcoded-wrapper">
                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">

                            <!-- Main-body start -->
                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-body">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                @RenderBody()
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Page body end -->
                                </div>
                            </div>
